<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>MvcMegaForms</id>
    <version>0.1.0.2</version>
    <authors>http://about.me/nootn</authors>
    <owners>http://about.me/nootn</owners>
    <projectUrl>http://nootn.github.com/MVC-Mega-Forms/</projectUrl>
